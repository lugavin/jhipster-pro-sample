<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gavin.myapp.repository.CommonConditionItemRepository">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.gavin.myapp.domain.CommonConditionItem">
        <id column="id" property="id"/>
        <result column="id" property="id"/>
        <result column="prefix" property="prefix"/>
        <result column="field_name" property="fieldName"/>
        <result column="field_type" property="fieldType"/>
        <result column="operator" property="operator"/>
        <result column="value" property="value"/>
        <result column="suffix" property="suffix"/>
        <result column="`order`" property="order"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,id,prefix,field_name,field_type,operator,value,suffix,`order`
    </sql>
</mapper>
