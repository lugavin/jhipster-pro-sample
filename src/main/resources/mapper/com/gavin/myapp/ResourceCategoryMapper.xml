<mapper namespace="com.gavin.myapp.repository.ResourceCategoryRepository">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.gavin.myapp.domain.ResourceCategory">
        <id column="id" property="id"/>
        <result column="id" property="id"/>
        <result column="title" property="title"/>
        <result column="code" property="code"/>
