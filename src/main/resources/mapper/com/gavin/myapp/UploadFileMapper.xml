<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gavin.myapp.repository.UploadFileRepository">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.gavin.myapp.domain.UploadFile">
        <id column="id" property="id"/>
        <result column="id" property="id"/>
        <result column="full_name" property="fullName"/>
        <result column="name" property="name"/>
        <result column="ext" property="ext"/>
        <result column="type" property="type"/>
        <result column="url" property="url"/>
        <result column="path" property="path"/>
        <result column="folder" property="folder"/>
        <result column="entity_name" property="entityName"/>
        <result column="create_at" property="createAt"/>
        <result column="file_size" property="fileSize"/>
        <result column="reference_count" property="referenceCount"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,id,full_name,name,ext,type,url,path,folder,entity_name,create_at,file_size,reference_count
    </sql>
</mapper>
